class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        nums.sort()
        k = 0
        for i in range(len(nums)-1):
            if nums[i] == nums[i+1]:
                nums[i] = 1000

        nums.sort()

        for i in nums:
            if i != 1000:
                k += 1

        return k
